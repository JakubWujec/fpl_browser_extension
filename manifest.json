{
    "manifest_version": 2,
    "name": "Borderify",
    "version": "1.0",
    "description": "Adds a red border to all webpages matching mozilla.org.",
    "icons": {
        "48": "icons/border-48.png",
        "96": "icons/border-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://fantasy.premierleague.com/my-team"
            ],
            "js": [
                "borderify.js"
            ],
            "run_at": "document_end"
        }
    ]
}